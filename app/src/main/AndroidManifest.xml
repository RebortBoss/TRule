<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jkt.rule">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".itemactivities.StandardActivity">
        </activity>
        <activity android:name=".itemactivities.IndexActivity">
        </activity>
        <activity
            android:name=".itemactivities.CustomSizeActivity"
            android:label="@string/title_activity_custom_size"
            android:theme="@style/MyTheme.NoActionBar">
        </activity>
        <activity android:name=".itemactivities.CustomColorActivity">
        </activity>
        <activity android:name=".itemactivities.CustomTextActivity">
        </activity>
        <activity android:name=".itemactivities.CustomNumActivity">
        </activity>
        <activity android:name=".itemactivities.CustomSenSpaceActivity">
        </activity>
        <activity android:name=".itemactivities.SetLocationActivity">
        </activity>
    </application>

</manifest>
